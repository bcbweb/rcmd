    "aws-amplify": "^5.2.2",
    "lit": "^2.7.2",
    "pwa-helper-components": "^0.2.10",
    "pwa-helpers": "^0.9.1",
    "urlpattern-polyfill": "^7.0.0",
    "workbox-build": "^6.5.2",
    "workbox-core": "^6.5.2",
    "workbox-precaching": "^6.5.2"
  },
